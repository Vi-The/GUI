<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="700.0" prefWidth="700.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gui.StartController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ImageView fitHeight="429.0" fitWidth="599.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../../../Assets/Background/TilStartSkærm.png" />
      </image>
   </ImageView>
   <Label prefHeight="29.0" prefWidth="450.0" text="Når du starter spillet tryk på H for at få hjælp">
      <font>
         <Font name="Arial Black" size="18.0" />
      </font>
   </Label>
   <Label layoutX="184.0" layoutY="125.0" text="Tryk på start for at starte spillet">
      <font>
         <Font size="24.0" />
      </font></Label>
    <Button fx:id="Startbutton" onAction="#clickStart" prefHeight="38.0" prefWidth="87.0" text="Start">
      <font>
         <Font size="18.0" />
      </font></Button>
   <Label text="Du bevæger dig med tasterne W, A, S, D">
      <font>
         <Font size="18.0" />
      </font>
   </Label>
</VBox>
